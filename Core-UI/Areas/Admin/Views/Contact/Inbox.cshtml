@model IEnumerable<EntityLayer.Concrete.Contact>

@{
    ViewData["Title"] = "Inbox";
    Layout = "~/Views/Shared/_AdminDashboard.cshtml";
}


<h1>Inbox</h1>
<div class="col-lg-12 animated fadeInRight">
    @foreach (var item in Model)
   {
       <div class="mail-box-header">
               
                <h2>
                   Kullanıcıdan Admine Gelen Mesaj
                </h2>
                <div class="mail-tools tooltip-demo m-t-md">


                    <h3>
                        <span class="font-normal">Konu: </span>@item.ContactSubject
                    </h3>
                    <h5>
                        <span class="float-right font-normal">@item.ContactDate</span>
                        <span class="font-normal">Kimden: </span>@item.ContactUserName
                    </h5>
                </div>
            </div>
                <div class="mail-box">


                <div class="mail-body">
                    <p>
                       
                       @item.ContactMessage
                        </p>
                   
                   
                </div>
                  
                       
                        <div class="clearfix"></div>


                </div>
       
   }
            
            </div>


